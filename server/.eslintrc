{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-plusplus": "off",
    "consistent-return": "off",
    "no-underscore-dangle": "off"
  },
  "parserOptions": {
    "ecmaVersion": 2017
  },

  "env": {
    "es6": true
  }
}
